{
  "parserOptions": {
    "ecmaVersion": 2015,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": false,
    "jquery": true
  },
  "globals": {
    "describe": false,
    "it": false
  },
  "extends": "eslint:recommended",
  "rules": {
    "no-console": "warn",
    "no-invalid-this": "warn",
    "no-undef": "error",
    "no-unused-vars": "warn",
    "no-var": "off",
    "strict": [
      2,
      "never"
    ]
  }
}
