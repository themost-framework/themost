{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jquery": true
  },
  "globals": {
    "describe": false,
    "it": false,
    "before": false,
    "beforeEach": false,
    "beforeAll": false,
    "after": false,
    "expect": false,
    "spyOn": false,
    "jasmine":false
  },
  "extends": "eslint:recommended",
  "rules": {
    "no-console": "off",
    "no-invalid-this": "warn",
    "no-undef": "error",
    "no-unused-vars": "warn",
    "no-var": [
      "error"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "strict": [
      2,
      "never"
    ]
  }
}
